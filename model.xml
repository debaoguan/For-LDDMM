<?xml version="1.0"?>
<model>
    <model-type>Registration</model-type>
    <dimension>3</dimension>

    <template>

        <object id="putamen">
            <deformable-object-type>SurfaceMesh</deformable-object-type>
            <attachment-type>Varifold</attachment-type>
            <noise-std>0.001</noise-std>
            <kernel-width>0.5</kernel-width>
            <kernel-type>keops</kernel-type>
            <filename>data/Vel_Cannie_surface_Heart.vtk</filename>
        </object>

    </template>

    <deformation-parameters>
        <kernel-width>2</kernel-width>
        <kernel-type>keops</kernel-type>
        <number-of-timepoints>25</number-of-timepoints>
    </deformation-parameters>

</model>
